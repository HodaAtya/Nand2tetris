/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
	
Not(in=sel,out=selComplement);
And(a=a,b=selComplement,out=aAnd);
And(a=sel,b=b,out=bAnd);
Or(a=aAnd ,b=bAnd ,out=out );

}